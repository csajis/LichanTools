<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox spacing="10.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label text="Ingrese nombre o rut">
         <font>
            <Font name="System Italic" size="13.0" />
         </font>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Label>
      <TextField fx:id="people_tab_filter" onAction="#filterPeopleTableAction" prefHeight="26.0" prefWidth="462.0">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </TextField>
      <TableView fx:id="people_tab" focusTraversable="false" prefHeight="208.0" prefWidth="462.0">
         <columns>
            <TableColumn fx:id="people_tab_name" prefWidth="311.0" text="Nombre" />
            <TableColumn fx:id="people_tab_rut" prefWidth="151.0" text="Rut" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </TableView>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
